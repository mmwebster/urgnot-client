{
  "rules": {
    ".read": "true",
    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid",
        "organizations": {
          "$organization": {
            ".read": "auth != null"
          }
        }
      }
    },
    "organizations": {
      ".read": "auth != null",
      "$organization": {
        ".write": "auth != null"
      }
    }
  }
}

